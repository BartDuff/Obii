<h1><%= @obii.name %></h1>
<%= image_tag(@obii.image, :alt => @obii.name, :class => "bigphoto") %>
<p><h2>User with this Obii: </h2><br/>
<div class='Obiiz' id='own'>
<ul>
	<% @users.each do |user| %>
	<li><%= image_tag(user.avatar, :alt => user.name, :class => "photo")  %></li>
	<% end %>
</ul>
</div>

<%= link_to 'Edit', edit_obii_path(@obii) %> |
<%= link_to 'Back', obiis_path %>
<%= link_to 'Destroy', @obii, method: :delete, data: { confirm: 'Are you sure?' } %>