<h1><%= @obii.name %></h1><br>
<%= image_tag(@obii.image_url, :alt => @obii.name, :class => "bigphoto") %>
<%= render 'add_delete_this_obii' %>
<p><h2>User with this Obii: </h2><br/>
<div class='Obiiz' id='own'>
<table>
<tr>
<% @obii.users.each do |user| %>
<td><%= image_tag(user.avatar_url, :class => 'photo', :title => user.name) %></td>
<% end %>
</tr>
</table>
</div>

<%= link_to 'Edit', edit_obii_path(@obii) %>
<%= link_to 'Back', obiis_path %>
<%= link_to 'Destroy', @obii, method: :delete, data: { confirm: 'Are you sure?' } %>