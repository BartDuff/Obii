<% i = 1 %>
<h1>All Obiiz</h1>
<table>
<tr>
  <% @obiis.each do |obii| %>
      <td><%= link_to image_tag(obii.image_url, :class => 'photo'), obii %><br>
      <%= obii.name %></td>
	<% if i != 0 %>
		<% if (i%8 == 0) && (i != @obiis.count-1) %>
		</tr><tr>
		<% else %>
		<% end %>
	<% end %>
	<% i += 1 %>
  <% end %>
</table>